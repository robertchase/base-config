[user]
	email = GIT-EMAIL
	name = bob chase
[alias]
    br = branch
    co = checkout
    st = status
    last = log -1 HEAD
    lol = log --graph --pretty=oneline --abbrev-commit --decorate
    loltop = !git lol HEAD~10..
    df = !gdf
    mt = mergetool --tool=vimdiff --no-prompt
    hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
[color]
    ui = true
[core]
	editor = /usr/bin/vim
[push]
	default = simple
